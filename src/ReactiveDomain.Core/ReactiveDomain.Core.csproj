
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <BuildWithNet40>true</BuildWithNet40>
  </PropertyGroup>
  <Import Project="../ci.build.imports" />
  <PropertyGroup>
    <TargetFrameworks>$(LibTargetFrameworks)</TargetFrameworks>
    <RootNamespace>ReactiveDomain</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>
  <Target Name="PreBuild" BeforeTargets="PreBuildEvent">
    <Exec Command="@echo off&#xD;&#xA;&#xD;&#xA;echo ..&#xD;&#xA;echo Perkin Elmer Build dir  is:  $(TRAVIS_BUILD_DIR)&#xD;&#xA;echo ..&#xD;&#xA;echo Perkin Elmer Branch is:  $(TRAVIS_BRANCH)&#xD;&#xA;&#xD;&#xA;echo Perkin Elmer Branch is:  $(TRAVIS_BRANCH)&#xD;&#xA;echo ..&#xD;&#xA;&#xD;&#xA;echo Continuous integration is:   $(CONTINUOUS_INTEGRATION)&#xD;&#xA;&#xD;&#xA;echo EVENT TYPE IS:   $(TRAVIS_EVENT_TYPE)&#xD;&#xA;&#xD;&#xA;echo Travis tag is: $(TRAVIS_TAG)&#xD;&#xA;&#xD;&#xA;echo Stability is: $(stable)&#xD;&#xA;&#xD;&#xA;echo..&#xD;&#xA;echo..&#xD;&#xA;&#xD;&#xA;" />
  </Target>
</Project>